<html><head>
        <title>{{title}}</title>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
        <script type="text/javascript" src="http://code.jquery.com/ui/1.11.2/jquery-ui.js"></script>

    <script src="https://raw.githubusercontent.com/furf/jquery-ui-touch-punch/master/jquery.ui.touch-punch.min.js"></script>
    <script src="/javascripts/bootstrap.min.js" type="text/javascript"></script>
    <script src="/javascripts/socket.io-1.2.0.js" type="text/javascript"></script>
    <script src="/javascripts/globalSocketio.js" type="text/javascript"></script>
        <link rel="stylesheet" href="/stylesheets/base.css">
    <link rel="stylesheet" href="/stylesheets/main-layout.css">
        <link href="http://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href='https://fonts.googleapis.com/css?family=Comfortaa' rel='stylesheet' type='text/css'>
      {{#each css}}
        <link rel="stylesheet" href="/stylesheets/{{this}}">
      {{/each}}
      {{#each js}}
        <script src="/javascripts/{{this}}" type="text/javascript"></script>
      {{/each}}
      </head>
      <body>
      {{#if user}}
        <div class="navbar navbar-default navbar-static-top navbar-default-active">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-ex-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand navbar-brand-active" href="/">
                      <span class="fa-stack fa-1x">
                        <i class="fa fa-send fa-stack-2x"></i>
                      </span>
                      μProject
                    </a>
                </div>
                <div class="collapse navbar-collapse" id="navbar-ex-collapse">
                    <ul class="nav navbar-nav navbar-right navbar-nav-active">
                        <li class="{{queuedSelected}}">
                            <a href="/queued">
                              <span class="fa-stack fa-1x">
                                <i class="fa fa-bolt fa-stack-2x"></i>
                              </span>
                              Queued
                            </a>
                        </li>
                        <li class="{{chatnowSelected}}">
                            <a href="/chat-room">
                              <span class="fa-stack fa-1x">
                                <i class="fa fa-comment fa-stack-2x"></i>
                                 <strong class="fa-stack-1x text-inverse usercount"><span class="ChatNowOnlineUserCount"></span></strong>
                              </span>
                              ChatNow
                            </a>
                        </li>
                        <li class="{{bugsSelected}}">
                            <a href="/bugs">
                              <span class="fa-stack fa-1x">
                                <i class="fa fa-bug fa-stack-2x"></i>
                              </span>
                              Bugs
                            </a>
                        </li>
                        <li class="dropdown">
                            <span id="username" class="hide">{{user.username}}</span>
                          <a href="#" class="dropdown-toggle nav-padding" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" role="button">Hi, {{#if user.displayName}}{{user.displayName}}{{else}}{{user.username}}{{/if}} <span class="caret"></span></a>
                          <ul class="dropdown-menu">
                            <li class="{{settingsSelected}}">
                              <a href="/settings">Settings <span class="fa fa-cog pull-right"></span></a>
                            </li>
                            <li role="separator" class="divider"></li>
                            <li>
                              <a href="/logout">Log out <span class="fa fa-sign-out pull-right"></span></a>
                            </li>
                          </ul>
                        </li>
                    </ul>
      {{else}}
            <div class="navbar navbar-default navbar-static-top">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-ex-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="/"><span class="fa fa-send">&nbsp;μProject</span></a>
                    </div>
                    <div class="collapse navbar-collapse" id="navbar-ex-collapse">
                        <ul class="nav navbar-nav navbar-right">
                            <li>
                              <a href="/auth/github"><span class="fa fa-github">&nbsp;Login with <strong>Github</strong></span></a>
                          </li>
                        </ul>
      {{/if}}

                </div>
            </div>
          <div id="newMessageAlert"></div>
        </div>
      {{{body}}}
      <br><br>
      <footer class="section section-black ">
          <div class="container ">
              <div class="row ">
                  <div class="col-xs-6 ">
                      <img src="/images/boston-university-logo.gif" class="img-responsive" alt="Boston University">
                      <br>
                      <p class="text-inverse "></p>
                  </div>
                  <div class="col-xs-6 ">
                    <p class="text-info"><span id="uProjectOnlineUserCount"></span> active member(s) online.</p>
                      <p class="text-info">This application is a project of One Team, CS 673.</p>
                  </div>
              </div>
          </div>
      </footer>
      </body>
</html>
